{
  "entrypoint": "index.html",
  "shell": "src/maximo-app.html",
  "fragments": [
    "src/maximo-login.html",
    "src/maximo-logout.html",
    "src/maximo-internal.html",
    "src/maximo-work-orders-list.html",
    "src/maximo-work-order.html",
    "src/maximo-not-found.html",
    "maximo-auth-ajax.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
